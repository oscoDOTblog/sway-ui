{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "BlogS3ImageUpload",
        "Effect": "Allow",
        "Action": ["s3:PutObject"],
        "Resource": ["arn:aws:s3:::sway-public-use2/blog/*"]
      },
      {
        "Sid": "BlogS3ImageRead",
        "Effect": "Allow",
        "Action": ["s3:GetObject"],
        "Resource": ["arn:aws:s3:::sway-public-use2/blog/*"]
      },
      {
        "Sid": "BlogS3ImageDelete",
        "Effect": "Allow",
        "Action": ["s3:DeleteObject"],
        "Resource": ["arn:aws:s3:::sway-public-use2/blog/*"]
      },
      {
        "Sid": "BlogS3BucketLocation",
        "Effect": "Allow",
        "Action": ["s3:GetBucketLocation"],
        "Resource": ["arn:aws:s3:::sway-public-use2"]
      }
    ]
  }